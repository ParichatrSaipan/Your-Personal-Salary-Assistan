{
  "buildCommand": "cd salary && npm install && npm run build",
  "outputDirectory": "salary/dist",
  "installCommand": "npm install --prefix ./salary",
  "devCommand": "cd salary && npm run dev",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/backend/app.py" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/backend/app.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
